{
  "name": "@aosplatform/debug-component-lib",
  "version": "2020.4.0-rc.3",
  "description": "UI component library",
  "license": "MIT",
  "author": "Pristine Phuc Tran",
  "main": "./dist/lib/index.js",
  "scripts": {
    "build": "rm -rf dist/lib/* && (npm run build:js)",
    "build:css": "npx parcel build src/lib/styles/index.scss -d ./dist/lib/assets/styles",
    "build:docs": "npx parcel build src/demo/index.js -d ./dist/docs && cp src/demo/index.html dist/docs/index.html",
    "build:js": "npm run lint:fix && ./node_modules/.bin/webpack --mode=production",
    "dev": "URI='' node ./server.js",
    "lint": "./node_modules/.bin/eslint src",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "py": "python src/mock-data/server.py",
    "serve": "http-server . --cors -p9000",
    "start": "rm -rf .cache/* && URI=http://localhost:9000 npx parcel src/demo/index.html",
    "stats": "webpack --profile --json > webpack-stats.json",
    "test": "./node_modules/.bin/jest",
    "test-watch": "jest --watch",
    "clean": "rm -rf *.tar.gz"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "setupFiles": [
      "./src/test/setupTest.js"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "is-react": "^1.3.3",
    "lodash": "^4.17.11",
    "match-sorter": "^2.3.0",
    "moment": "^2.24.0",
    "prop-types": "^15.6.2",
    "querystring": "^0.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fontawesome": "^1.6.1",
    "react-iframe": "^1.3.3",
    "react-json-tree": "^0.11.0",
    "react-jsonschema-form": "^1.0.5",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-render-html": "^0.6.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "2.1.1",
    "react-spinners": "^0.5.4",
    "react-table": "6.8.6",
    "react-toastify": "^4.5.2",
    "reactstrap": "^6.5.0",
    "recharts": "^1.4.2",
    "recompose": "^0.30.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-logic": "^2.0.2",
    "redux-rest-resource": "^0.19.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "csv-loader": "^3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "escope": "^3.6.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "http-proxy-middleware": "^0.19.0",
    "http-server": "^0.11.1",
    "husky": "^3.0.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.4",
    "parcel-bundler": "^1.10.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "react-dev-utils": "^6.0.5",
    "sass": "^1.14.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "warning": "^4.0.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.6.0",
    "xml-loader": "^1.2.1"
  },
  "peerDependencies": {
    "moment": ">=2.24.0",
    "prop-types": ">=15.5.10",
    "react": ">=16.0.1",
    "react-dom": ">=16.0.1",
    "react-redux": ">=5.0.1",
    "recompose": ">=0.25.0",
    "redux": ">=3.6.0",
    "redux-thunk": ">=2.2.0"
  },
  "optionalDependencies": {},
  "publishConfig": {
    "registry": "https://artifacts.apple.com/api/npm/aosplatform-npm-local/"
  },
  "alias": {
    "src": "./src"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint src --fix",
      "git add"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier --no-semi --single-quote --write"
    ]
  },
  "proxy": "http://localhost:9000",
  "sideEffects": false
}
